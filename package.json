{
  "name": "simple-icons-website",
  "version": "1.0.0",
  "description": "Website for Simple Icons",
  "private": "true",
  "repository": {
    "type": "git",
    "url": "git@github.com:simple-icons/simple-icons-website.git"
  },
  "bugs": {
    "url": "https://github.com/simple-icons/simple-icons-website/issues"
  },
  "author": "Simple Icons Collaborators",
  "license": "CC0-1.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "postinstall": "npm run setup:husky && npm run setup:blobstream",
    "setup:husky": "node -e 'process.exit(process.env.CI === undefined)' || husky install",
    "setup:blobstream": "node -e 'require(\"fs\").copyFileSync(\"node_modules/blob-stream/.js\", \"node_modules/blob-stream/blob-stream.js\")'",
    "fetch-deprecated-icons": "ts-node --esm scripts/fetch-deprecated-icons.ts",
    "create-sitemap": "ts-node --esm scripts/create-sitemap.ts",
    "create-cname": "ts-node --esm scripts/create-cname.ts"
  },
  "devDependencies": {
    "@playwright/test": "1.36.2",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "anywhere": "1.6.0",
    "autoprefixer": "10.4.14",
    "esbuild": "0.18.17",
    "eslint": "8.48.0",
    "eslint-config-simple-icons": "0.1.2",
    "husky": "8.0.3",
    "postcss": "8.4.27",
    "postcss-import": "15.1.0",
    "prettier": "3.0.0",
    "simple-icons": "9.16.1",
    "stylelint": "15.10.2",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-tailwindcss": "0.0.7",
    "tailwindcss": "3.3.3",
    "ts-node": "10.9.1"
  },
  "dependencies": {
    "blob-stream": "0.1.3",
    "fast-fuzzy": "1.12.0",
    "pdfkit": "0.13.0"
  }
}
